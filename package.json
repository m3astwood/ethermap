{
  "name": "ethermap",
  "private": true,
  "version": "1.0.0",
  "description": "> This is very much a janky _earlydays_ project. All help is welcome!",
  "type": "module",
  "scripts": {
    "start": "tsx server.ts",
    "dev": "NODE_ENV=development nodemon -e js,ts,json --exec npm start",
    "build:backend": "tsc --project ./backend/tsconfig.back.json && tsc-alias -p ./backend/tsconfig.back.json",
    "build:frontend": "vite build",
    "build": "npm run build:backend && npm run build:frontend",
    "jest": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
    "test:all": "npm run jest",
    "test:routes": "npm run jest -- ./backend/tests/routes.ts",
    "test:db": "npm run jest -- ./backend/tests/db.ts",
    "knex:run": "tsx ./node_modules/.bin/knex --knexfile knexfile.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:up": "drizzle-kit up",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@lit/task": "^1.0.3",
    "@ngneat/effects": "^2.1.2",
    "@ngneat/elf": "^2.5.1",
    "@ngneat/elf-entities": "^5.0.2",
    "@ngneat/elf-persist-state": "^1.2.1",
    "@thepassle/app-tools": "^0.10.0",
    "connect-session-knex": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.2",
    "elf": "^0.1.22",
    "exome": "^2.8.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "leaflet": "^1.9.4",
    "leaflet-contextmenu": "^1.4.0",
    "lit": "^3.3.1",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "urlpattern-polyfill": "^10.1.0",
    "vite-express": "^0.21.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.5",
    "@ngneat/elf-devtools": "^1.3.0",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.20",
    "@types/pg": "^8.15.4",
    "@types/supertest": "^6.0.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.31.0",
    "eslint-plugin-lit": "^2.1.1",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.5"
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "inlineStoreInClass": false,
      "repoLibrary": "frontend/store",
      "plugins": []
    }
  },
  "nodemon": {
    "execMap": {
      "ts": "ts-node"
    }
  }
}
