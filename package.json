{
  "name": "ethermap",
  "private": true,
  "version": "1.0.0",
  "description": "> This is very much a janky _earlydays_ project. All help is welcome!",
  "type": "module",
  "scripts": {
    "start": "tsx server.ts",
    "dev": "NODE_ENV=development nodemon -e js,ts,json --exec npm start",
    "build:backend": "tsc --project ./backend/tsconfig.back.json && tsc-alias -p ./backend/tsconfig.back.json",
    "build:frontend": "vite build",
    "build": "npm run build:backend && npm run build:frontend",
    "jest": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
    "test:all": "npm run jest",
    "test:routes": "npm run jest -- ./backend/tests/routes.ts",
    "test:db": "npm run jest -- ./backend/tests/db.ts",
    "knex:run": "tsx ./node_modules/.bin/knex --knexfile knexfile.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:up": "drizzle-kit up",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@lit/task": "^1.0.0",
    "@ngneat/effects": "^2.1.2",
    "@ngneat/elf": "^2.5.1",
    "@ngneat/elf-entities": "^5.0.1",
    "@ngneat/elf-persist-state": "^1.2.1",
    "@thepassle/app-tools": "^0.9.8",
    "connect-session-knex": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.36.0",
    "drizzle-zod": "^0.5.1",
    "elf": "^0.1.22",
    "exome": "^2.0.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "leaflet": "^1.9.4",
    "leaflet-contextmenu": "^1.4.0",
    "lit": "^3.0.0",
    "pg": "^8.13.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "urlpattern-polyfill": "^9.0.0",
    "vite-express": "^0.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.2.12",
    "@ngneat/elf-devtools": "^1.3.0",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/leaflet": "^1.9.12",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.27.0",
    "eslint": "^8.48.0",
    "eslint-plugin-lit": "^1.10.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.10"
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "inlineStoreInClass": false,
      "repoLibrary": "frontend/store",
      "plugins": []
    }
  },
  "nodemon": {
    "execMap": {
      "ts": "ts-node"
    }
  }
}
