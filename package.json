{
  "name": "ethermap",
  "private": true,
  "version": "1.0.0",
  "description": "> This is very much a janky _earlydays_ project. All help is welcome!",
  "type": "module",
  "scripts": {
    "start": "tsx server.ts",
    "dev": "nodemon -e js,ts,json --exec npm start",
    "build:backend": "tsc --project ./backend/tsconfig.back.json && tsc-alias -p ./backend/tsconfig.back.json",
    "build:frontend": "vite build",
    "build": "npm run build:backend && npm run build:frontend",
    "ava": "npm run build:backend && ava",
    "test:all": "npm run ava",
    "test:routes": "npm run ava -- ./backend/tests/routes.ts",
    "test:db": "npm run ava -- ./backend/tests/db.ts",
    "knex:run": "tsx ./node_modules/.bin/knex --knexfile knexfile.ts",
    "migrate:latest": "npm run knex:run -- migrate:latest",
    "migrate:rollback": "npm run knex:run -- migrate:rollback",
    "migrate:down": "npm run knex:run -- migrate:down",
    "migrate:drop": "npm run knex:run -- migrate:rollback --all"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@lit/task": "^1.0.0",
    "@ngneat/effects": "^2.1.2",
    "@ngneat/elf": "^2.5.1",
    "@ngneat/elf-entities": "^5.0.1",
    "@ngneat/elf-persist-state": "^1.2.1",
    "@thepassle/app-tools": "^0.9.8",
    "connect-session-knex": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "elf": "^0.1.22",
    "exome": "^2.0.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "knex": "^2.1.0",
    "leaflet": "^1.9.4",
    "leaflet-contextmenu": "^1.4.0",
    "lit": "^3.0.0",
    "objection": "^3.1.1",
    "pg": "^8.11.3",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sqlite3": "^5.1.6",
    "urlpattern-polyfill": "^9.0.0",
    "vite-express": "^0.10.0"
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@ngneat/elf-devtools": "^1.3.0",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/leaflet": "^1.9.9",
    "ava": "^5.3.1",
    "eslint": "^8.48.0",
    "eslint-plugin-lit": "^1.10.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.10"
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "inlineStoreInClass": false,
      "repoLibrary": "frontend/store",
      "plugins": []
    }
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "backend/": "dist/backend/"
      },
      "compile": false,
      "extensions": [
        "ts"
      ]
    },
    "environmentVariables": {
      "NODE_ENV": "test"
    }
  },
  "nodemon": {
    "execMap": {
      "ts": "ts-node"
    }
  }
}
